<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.shape.Arc?>

<AnchorPane prefHeight="768.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.N.views.MattController">
   <children>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="30.0" layoutX="558.0" layoutY="221.0" prefHeight="200.0" prefWidth="200.0" vgap="10.0">
         <children>
            <Arc fx:id="fillArc" fill="DODGERBLUE" length="270.0" radiusX="100.0" radiusY="100.0" startAngle="45.0" stroke="BLACK" strokeType="INSIDE" type="ROUND" />
            <Slider fx:id="slider" blockIncrement="45.0" majorTickUnit="90.0" max="360.0" minorTickCount="30" onMouseReleased="#sliderHandler" showTickMarks="true" />
            <ColorPicker fx:id="colorPicker" onAction="#colorPickHandle" promptText="Pick background" />
            <Button fx:id="btn_goToMain" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#handleOnClick" text="Main Menu" />
         </children>
      </FlowPane>
   </children>
</AnchorPane>
